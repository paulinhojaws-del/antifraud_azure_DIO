# üß† An√°lise de Documentos Antifraude com Azure AI

import requests, json

# Configura√ß√µes do Azure
endpoint = "https://<seu-recurso>.cognitiveservices.azure.com/"
key = "<sua-chave>"

# Caminho do arquivo de teste
arquivo = "samples/recibo_exemplo1.pdf"

# URL do modelo de recibos predefinido
url = f"{endpoint}formrecognizer/documentModels/prebuilt-receipt:analyze?api-version=2023-07-31"

headers = {
    "Ocp-Apim-Subscription-Key": key,
    "Content-Type": "application/pdf"
}

# Envia o arquivo para an√°lise
with open(arquivo, "rb") as f:
    response = requests.post(url, headers=headers, data=f)

# Exibe o resultado formatado
resultado = response.json()
print(json.dumps(resultado, indent=2, ensure_ascii=False))
